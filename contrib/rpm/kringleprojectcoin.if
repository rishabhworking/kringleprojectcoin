##	</summary>
## </param>
#
interface(`kringleprojectcoin_search_lib',`
	gen_require(`
		type kringleprojectcoin_var_lib_t;
	')

	allow $1 kringleprojectcoin_var_lib_t:dir search_dir_perms;
	files_search_var_lib($1)
')

