
## <summary>policy for kringleprojectcoin</summary>


########################################
## <summary>
##	Transition to kringleprojectcoin.
## </summary>
## <param name="domain">
## <summary>
##	Domain allowed to transition.
## </summary>
## </param>
#
interface(`kringleprojectcoin_domtrans',`
	gen_require(`
		type kringleprojectcoin_t, kringleprojectcoin_exec_t;
	')

	corecmd_search_bin($1)
	domtrans_pattern($1, kringleprojectcoin_exec_t, kringleprojectcoin_t)
')


########################################
## <summary>
##	Execute kringleprojectcoin server in the kringleprojectcoin domain.
## </summary>
## <param name="domain">
##	<summary>
##	Domain allowed access.
##	</summary>
## </param>
#
interface(`kringleprojectcoin_initrc_domtrans',`
	gen_require(`
		type kringleprojectcoin_initrc_exec_t;
	')

	init_labeled_script_domtrans($1, kringleprojectcoin_initrc_exec_t)
')


########################################
## <summary>
##	Search kringleprojectcoin lib directories.
## </summary>
## <param name="domain">
##	<summary>
