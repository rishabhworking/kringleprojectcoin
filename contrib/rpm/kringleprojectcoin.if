########################################
## <summary>
##	All of the rules required to administrate
##	a kringleprojectcoin environment
## </summary>
## <param name="domain">
##	<summary>
##	Domain allowed access.
##	</summary>
## </param>
## <param name="role">
##	<summary>
##	Role allowed access.
##	</summary>
## </param>
## <rolecap/>
#
interface(`kringleprojectcoin_admin',`
	gen_require(`
		type kringleprojectcoin_t;
		type kringleprojectcoin_initrc_exec_t;
		type kringleprojectcoin_var_lib_t;
	')

	allow $1 kringleprojectcoin_t:process { ptrace signal_perms };
	ps_process_pattern($1, kringleprojectcoin_t)

	kringleprojectcoin_initrc_domtrans($1)
	domain_system_change_exemption($1)
	role_transition $2 kringleprojectcoin_initrc_exec_t system_r;
	allow $2 system_r;

	files_search_var_lib($1)
	admin_pattern($1, kringleprojectcoin_var_lib_t)

')

